public class BarkingDog {
    public static void main(String[ ] args) {


        System.out.println(isPalindrome(-121));



    }


    public static boolean isPalindrome(int number){
        int reverse=0, lastDigit=0;
        int check=number;

        while(number!=0) {
            lastDigit = number % 10;
            reverse = reverse * 10 + lastDigit;
            number = number / 10;
        }
        
        if(check==reverse)
            return true;
        else {return false;}


    }
}







